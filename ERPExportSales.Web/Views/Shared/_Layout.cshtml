<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="utf-8">
    @*<meta name="viewport" content="width=device-width, initial-scale=1">*@
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Shanghai Prime Machinery Co.,Ltd. Export Service Platform</title>
    @*<link rel="stylesheet" type="text/css"
              href="https://maxcdn.bootstrapcdn.com/bootswatch/3.3.7/flatly/bootstrap.min.css" />
        <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/stylesheets/bootstrap.extend.css")" />
        <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/stylesheets/font-awesome.min.css")" />
        <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/stylesheets/semantic.min.css")" />*@
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="@Url.Content("~/Scripts/bower_components/bootstrap/dist/css/bootstrap.min.css")">
    <link href="@Url.Content("~/Content/css/scroll.css")"  rel="stylesheet" />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="@Url.Content("~/Scripts/bower_components/font-awesome/css/font-awesome.min.css")">
    <!-- Ionicons -->
    <link rel="stylesheet" href="@Url.Content("~/Scripts/bower_components/Ionicons/css/ionicons.min.css")">
    <!-- DataTables -->
    <link rel="stylesheet" href="@Url.Content("~/Scripts/bower_components/datatables.net-bs/css/dataTables.bootstrap.min.css")">
    <!-- Theme style -->
    <link rel="stylesheet" href="@Url.Content("~/Scripts/dist/css/AdminLTE.min.css")">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="@Url.Content("~/Scripts/dist/css/skins/_all-skins.min.css")">

    @RenderSection("Css", false)
    <script src="@Url.Content("~/Scripts/jquery-3.3.1.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")"></script>
    <!-- jQuery 3 -->
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.js")" type="text/javascript"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="@Url.Content("~/Scripts/bower_components/bootstrap/dist/js/bootstrap.min.js")"></script>
    <!-- DataTables -->
    <script src="@Url.Content("~/Scripts/bower_components/datatables.net/js/jquery.dataTables.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bower_components/datatables.net-bs/js/dataTables.bootstrap.min.js")"></script>
    <!-- SlimScroll -->
    <script src="@Url.Content("~/Scripts/bower_components/jquery-slimscroll/jquery.slimscroll.min.js")"></script>
    <!-- FastClick -->
    <script src="@Url.Content("~/Scripts/bower_components/fastclick/lib/fastclick.js")"></script>
    <!-- AdminLTE App -->
    <script src="@Url.Content("~/Scripts/dist/js/adminlte.min.js")"></script>
    <script src="@Url.Content("~/Scripts/layout.js")"></script>
    <script src="@Url.Content("~/Scripts/echarts.js")"></script>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    @*<script src="http://cdn.static.runoob.com/libs/bootstrap/3.3.7/js/bootstrap.min.js"></script>*@
    <style type="text/css">
        container {
            width: 1500px;
            max-width: none !important;
        }
        html{
            overflow:scroll;
        }
    </style>
    <script type="text/javascript">
        function linkout() {
            window.location.href = "/Account/Logout";
        }
    </script>
</head>
<body class="hold-transition skin-blue layout-top-nav" style="padding-top:0px">
    <div class="wrapper">
        <header class="main-header">
            <nav class="navbar navbar-static-top">
                <div class="container">
                    <div class="navbar-header">
                        <a href="/Home/Index" class="navbar-brand"><b>Export Service</b>  Platform</a>
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
                            <i class="fa fa-bars"></i>
                        </button>
                    </div>

                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse pull-left" id="navbar-collapse">
                        <ul class="nav navbar-nav">
                            <li><a href="/Home/Index">Home</a></li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">System Setting <span class="caret"></span></a>
                                <ul class="dropdown-menu" role="menu">
                                    <li><a href="#" data-toggle="modal" data-target="#modal-default">Logout</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <!-- /.navbar-collapse -->
                    <!-- Navbar Right Menu -->
                    <div class="navbar-custom-menu">
                        <ul class="nav navbar-nav">
                            <!-- User Account Menu -->
                            <li class="dropdown user user-menu">
                                <!-- Menu Toggle Button -->
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <!-- hidden-xs hides the username on small devices so only the image appears. -->
                                    <span class="hidden-xs">Welcome,@ViewBag.NickName</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                    <!-- /.navbar-custom-menu -->
                </div>
                <!-- /.container-fluid -->
            </nav>
        </header>
        <!-- Full Width Column -->
        <div class="content-wrapper">
            <div class="container" style="width:1500px">
                <!-- Main content -->
                @RenderBody()
                <!-- /.content -->
            </div>
            <!-- /.container -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="container">
                <div class="pull-left hidden-xs">
                    <img src="../../Content/img/logo3.gif" />
                </div>
                <div class="pull-right hidden-xs">
                    <b>Version</b> 1.0.0
                </div>
                <strong>
                    Address:
                </strong>15 FLOOR,600 HENG FENG ROAD SHANGHAI
                <strong>
                    CHINA POST CODE:
                </strong>200070<br />
                <strong>
                    Tel:
                </strong>(86-21)63177707,63178900 Fax:(86-21)63178909,63177538
                <strong>
                    Email:
                </strong>export@sfiec.com; sfc@sfiec.com

                <div class="center hidden-xs">
                    <strong> Copyright:Shanghai Prime Machinery Co.,Ltd.</strong> All rights reserved.
                </div>
            </div>
            <!-- /.container -->
        </footer>
    </div>
    <div class="modal fade" id="modal-default">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h4 class="modal-title">Tips</h4>
                </div>
                <div class="modal-body">
                    <p>Do you want to quit the system?&hellip;</p>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary" onclick="linkout()">Yes</button>
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>
