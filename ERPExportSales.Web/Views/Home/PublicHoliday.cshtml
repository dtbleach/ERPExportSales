@model IList < ERPExportSales.Web.Models.PublicHolidayViewModel>

<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title">Public Holiday</h3>
    </div>
    <div class="box-body box-profile">
        <div class="box">
            @*<div class="box-header">
                    <h3 class="box-title">数据信息</h3>
                </div>*@
            <!-- /.box-header -->
            <div class="box-body">
                <div id="example1_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
                    <div class="row">
                        <div class="col-sm-12">
                            <table id="tbHoliday" class="table table-bordered table-striped dataTable" role="grid">
                                <thead>
                                    <tr role="row"><th>Holiday</th><th>Date</th></tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr role="row">
                                            <td class="sorting_1">@item.Holiday</td>
                                            <td>@item.Date</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.box-body -->
        </div>
    </div>
    <!-- /.box-body -->
</div>